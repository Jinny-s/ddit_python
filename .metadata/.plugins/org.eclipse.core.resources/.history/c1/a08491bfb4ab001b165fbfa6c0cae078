import random
r = random.randint(1, 10)

mine = input("홀/짝을 선택하세요")
print(mine)

if r%2 == 0:
    a = "짝"
    # print("랜덤 : " + str(r) + a)
else:
    a = "홀"
    # print("랜덤 : " + str(r) + a)

print("유저 : " + mine + " / 컴퓨터 : " + a + srt(r))

if mine == a:
    print("유저 우승!")
else:
    print("컴퓨터 우승!")